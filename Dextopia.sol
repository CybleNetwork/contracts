    function applyBuyFees() private {
        _taxFee = buyTaxFee;        
        _marketFee = buyMarketingFee;
        _liquidityFee = buyLiquidityFee;
    }

    function applySellFees() private {
        _taxFee = sellTaxFee;        
        _marketFee = sellMarketingFee;
        _liquidityFee = sellLiquidityFee;
    }
